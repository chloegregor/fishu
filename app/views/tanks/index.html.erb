<div class="navbar" style="justify-content: center;position: absolute;top: 0%;">
  <div class="logo">
      <%= image_tag 'fishu/logo.png' %>
  </div>
</div>
<div class="index">
<% if @user.tanks %>
  <% @tanks.each do |tank| %>

    <%= link_to tank.fish.last.name, tank_path(tank), class:"lien" %>
  <% end %>
<% end %>
<%= link_to 'Creer un nouvel aquarium', new_tank_path, class:"lien" %>
<%= button_to create_tank_path(@user), class:"btn"%>
</div>
